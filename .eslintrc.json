{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "ignorePatterns": ["public"],
  "plugins": ["react", "cypress"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "cypress/globals": true,
    "node": true
  }
}
